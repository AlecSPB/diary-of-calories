<?xml version = "1.0" encoding = "utf-8" ?>
<selector
	xmlns:android = "http://schemas.android.com/apk/res/android">
	<item>
		<shape>
			<gradient
				android:startColor = "#343434"
				android:endColor = "#171717"
				android:angle = "270" />
			<stroke
				android:width = "3dp"
				android:color = "#f3ae1b" />
			<corners
				android:radius = "5dp" />
			<padding
				android:left = "7dp"
				android:top = "3dp"
				android:right = "7dp"
				android:bottom = "5dp" />
		</shape>
	</item>
</selector>

